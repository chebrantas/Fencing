@model IEnumerable<Fektavimasis.Models.ViewModel.ParticipantsInfoViewModel>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResultsId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstParticipantNameSurname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Piercing)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Received)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecondParticipantNameSurname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Round)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ResultsId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstParticipantNameSurname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Piercing)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Received)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondParticipantNameSurname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Round)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditMatchup", new {id=item.ResultsId }) |
            @Html.ActionLink("Delete", "DeleteMatchup", new { id = item.ResultsId })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
